                                   icon: 'error'
                                 });
                               }
                            } catch (error) {
                             console.error('Error generating ExeIO link:', error);
                             Swal.fire({
                               title: 'Error',
                               text: 'Error generating ExeIO link. Please try again later.',
                               icon: 'error'
                             });
                           }
                          });
                        </script>
                        </div>
            </div>
          </div>
        </div>
            <% } %>
            <%- include('./components/ads') %>
            <h2> Join for Rewards</h2>
            <p class="text-gray">Join discord servers and get extra credits.</p>
             <div class="row">
               <% for (let i = 0, len = settings.api.client.j4r.ads.length; i < len; i++) { %>
                 <div class="col-md-6 grid-margin stretch-card">
                   <div class="card">
                     <div style="background-color: #0a0a0a3f; border: think solid white;" class="card-body">
                       <h4 class="card-title"><%= settings.api.client.j4r.ads[i].name %></h4>
                       <p class="card-description"> Join this server to receive: <br> <%= settings.api.client.j4r.ads[i].coins %> Credits </p>
                       <form class="forms-sample">
                         <a style="background-color: #0a0a0a3f;" role="button" class="btn btn-primary btn-lg" href="<%= settings.api.client.j4r.ads[i].invite %>" target="_blank">Join Server</a> <a style="background-color: #0a0a0a3f;" class="btn btn-primary btn-lg" href="../login">Claim reward</a>
                       </form>
                     </div>
                   </div>
                 </div>
               <% } %>
             </div>
           </div>
            <!-- content-wrapper ends -->
          <%- include('./components/footer') %>
          </div>
          <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
      </div>
      <!-- container-scroller -->
      <!-- plugins:js -->
      <script src="../../assets/vendors/js/vendor.bundle.base.js"></script>
      <!-- endinject -->
      <!-- Plugin js for this page -->
      <script src="../../assets/vendors/select2/select2.min.js"></script>
      <script src="../../assets/vendors/typeahead.js/typeahead.bundle.min.js"></script>
