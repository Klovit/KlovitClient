                                 });
                               } else {
                                 Swal.fire({
                                   title: 'Error',
                                   text: 'Error generating LinkPays link. Please try again later.',
                                   icon: 'error'
                                 });
                               }
                            } catch (error) {
                             console.error('Error generating LinkPays link:', error);
                             Swal.fire({
                               title: 'Error',
                               text: 'Error generating LinkPays link. Please try again later.',
                               icon: 'error'
                             });
                           }
                          });
                        </script>
                        </div>
            </div>
          </div>
        </div>
            <% } %>
            <% if (settings.exeio.enabled) { %>
              <h2>ExeIO</h2>
           <p class="text-gray">Earn coins by completing ExeIO links.</p>
            <% if (req.query.err) { %>
              <div style="border-radius: 15px;" class="alert">
                  <% if (req.query.err == "MUSTCOMPLETECAPTCHA") { %>
                    You must complete the captcha to prove you are human!
                <% } else if (req.query.err == "INVALIDCAPTCHARESPONSE") { %>
