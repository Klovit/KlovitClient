<html lang="en">
  <%- include('./components/head') %>
    <body>
      <script src="https://www.google.com/recaptcha/api.js" async defer></script>
      <div style="background-color: #0a0a0a3f" class="container-scroller">
        <!-- partial:partials/_sidebar.html -->
      <%- include('./components/sidebar') %>
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
          <!-- partial:partials/_navbar.html -->
           <%- include('./components/topnav') %>
          <!-- partial -->
        <div style="background-color: #0a0a0a3f" class="main-panel">
          <div style="background-color: #0a0a0a3f" class="content-wrapper">
            <div class="card-body" id="loading-panel">
              <center><h3>Loading...</h3></center>
            </div>
            <% if (settings.linkvertise.enabled) { %>
           <h2>Linkvertise</h2>
           <p class="text-gray">Earn coins by completing Linkvertise links.</p>
            <% if (req.query.err) { %>
              <div style="border-radius: 15px;" class="alert">
                  <% if (req.query.err == "MUSTCOMPLETECAPTCHA") { %>
                    You must complete the captcha to prove you are human!
                <% } else if (req.query.err == "INVALIDCAPTCHARESPONSE") { %>
                    You submitted an invalid captcha response, try again.
                <% } %>
              </div>
            <% } %>
            <% if (req.query.success) { %>
              <div class="alert" style="background-color: #00b950; border-radius: 15px;">
                Success! <%= settings.linkvertise.coins%> coins were added to your account.
            </div>
            <% } %>
              <div class="row">
                <div class="col-md-12 grid-margin stretch-card">
                    <div class="card">
                    <div style="background-color: #0a0a0a3f;" class="card-body" style="display: none;" id="generate-panel">
                      <p>Every time you complete a linkvertise link, you will earn <%= settings.linkvertise.coins%> coins!<br>Generate a link below:<br><br>
                        <button style="background-color: #0a0a0a3f;" class="btn btn-primary btn-lg" onclick="generatelv()">Generate Link</button></p></center>
                      </div>
                  <script type="text/javascript">
                    function generatelv() {
                      window.location.replace(`/earn/lv/gen`)
                    }
                  </script>
              </div>
            </div>
            </div>
            <% } %>
            <% if (settings.atglinks.enabled) { %>
              <h2>ATGLinks</h2>
           <p class="text-gray">Earn coins by completing ATGLinks links.</p>
            <% if (req.query.err) { %>
              <div style="border-radius: 15px;" class="alert">
                  <% if (req.query.err == "MUSTCOMPLETECAPTCHA") { %>
                    You must complete the captcha to prove you are human!
                <% } else if (req.query.err == "INVALIDCAPTCHARESPONSE") { %>
                    You submitted an invalid captcha response, try again.
                <% } %>
              </div>
            <% } %>
            <% if (req.query.success) { %>
              <div class="alert" style="background-color: #00b950; border-radius: 15px;">
                Success! <%= settings.atglinks.coins%> coins were added to your account.
            </div>
            <% } %>
              <div class="row">
                <div class="col-md-12 grid-margin stretch-card">
                    <div class="card">
                    <div style="background-color: #0a0a0a3f;" class="card-body" style="display: none;" id="generate-panel">
                      <p>Every time you complete a ATGLinks link, you will earn <%= settings.atglinks.coins%> coins!<br>Generate a link below:<br><br>
                        <button style="background-color: #0a0a0a3f;" id="generateatgBtn" class="btn btn-primary btn-lg">Generate Link</button></p></center>
                        <script type="text/javascript">
                                const generateBtn = document.getElementById('generateatgBtn');
                             generateBtn.addEventListener('click', async () => {
                            try {
                               const response = await fetch('/earn/atg/gen');
                               const data = await response.json();
                               if (response.ok) {
                                 Swal.fire({
                                   title: 'Success',
                                   text: 'ATGLinks link generated successfully.',
                                   icon: 'success',
                                   html: `Your ATGLinks link: <a style="color: aqua" href="${data.link}"> Link</a>`
                                 });
                               } else {
                                 Swal.fire({
                                   title: 'Error',
                                   text: 'Error generating ATGLinks link. Please try again later.',
                                   icon: 'error'
                                 });
                               }
                            } catch (error) {
                             console.error('Error generating ATGLinks link:', error);
                             Swal.fire({
                               title: 'Error',
                               text: 'Error generating ATGLinks link. Please try again later.',
                               icon: 'error'
                             });
                           }
                          });
                        </script>
                        </div>
            </div>
          </div>
        </div>
            <% } %>
            <% if (settings.linkpays.enabled) { %>
