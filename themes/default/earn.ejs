                <% } %>
              </div>
            <% } %>
            <% if (req.query.success) { %>
              <div class="alert" style="background-color: #00b950; border-radius: 15px;">
                Success! <%= settings.exeio.coins%> coins were added to your account.
            </div>
            <% } %>
              <div class="row">
                <div class="col-md-12 grid-margin stretch-card">
                    <div class="card">
                    <div style="background-color: #0a0a0a3f;" class="card-body" style="display: none;" id="generate-panel">
                      <p>Every time you complete a ExeIO link, you will earn <%= settings.exeio.coins%> coins!<br>Generate a link below:<br><br>
                        <button style="background-color: #0a0a0a3f;" id="generateexeioBtn" class="btn btn-primary btn-lg">Generate Link</button></p></center>
                        <script type="text/javascript">
                                const generateexeioBtn = document.getElementById('generateexeioBtn');
                             generatelpBtn.addEventListener('click', async () => {
                            try {
                               const response = await fetch('/earn/exeio/gen');
                               const data = await response.json();
                               if (response.ok) {
                                 Swal.fire({
                                   title: 'Success',
                                   text: 'ExeIO link generated successfully.',
                                   icon: 'success',
